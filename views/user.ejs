<ul class="list-group">
  <% users.forEach(user => { %>
    <li class="list-group-item d-flex justify-content-between align-items-center">
      <span><%= user.username %> - <%= user.email %></span>
      <button class="btn btn-primary btn-sm">Details</button> <!-- Example button -->
    </li>
  <% }) %>
</ul>
